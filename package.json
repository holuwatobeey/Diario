    "cross-env": "^5.1.5",
    "debug": "^3.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "helmet": "^3.12.1",
    "morgan": "^1.9.0",
    "winston": "2.4.0"
